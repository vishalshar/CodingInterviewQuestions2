public class RemoveOccurence 
{
    public static void main(String args[])
    {
        System.out.println(remove("abccddeeffgh", 'e'));
    }
    public static String remove(String s, char ch)
    {
        if(s == null)
            return null;

        String result = "";
        for(int i = 0;i<s.length();i++)
        {
            if(!(s.charAt(i) == ch))
                result+=s.charAt(i);
        }
        return result;
    }
}
